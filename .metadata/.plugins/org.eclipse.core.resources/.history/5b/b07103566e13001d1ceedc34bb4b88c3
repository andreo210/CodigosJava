package classe;

public class DataTEST {


	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Data d1 = new Data();
		d1.dia = 17;
		d1.mes = 2;
		d1.ano = 1984;
		
		Data d2 = new Data();
		d2.dia = 25;
		d2.mes = 8;
		d2.ano = 2011;
		
		System.out.printf("%d/%d/%d\n",d1.dia, d1.mes, d1.ano);
		System.out.printf("%d/%d/%d\n",d2.dia, d2.mes, d2.ano);

	}

}
